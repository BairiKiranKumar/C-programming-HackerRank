#include <stdio.h>
int main() {
    int a, b, c, d;
    int ans;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    if(a>b && a>c && a>d){
        ans = a;
    }
     else if(b>a && b>c && b>d){
        ans = b;
    }
    else if(c>a && c>b && c>d){
        ans = c;
    }
    else if(d>a && d>c && d>b){
        ans = d;
    } 
    printf("%d",ans);
    return 0;
}
